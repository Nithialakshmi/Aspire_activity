    <section class="dashboard">

    <div class="dash-content" >
        <div class="overview">
            <div class="studentDetail"> 
            <div class="title">
                <i class="uil uil-tachometer-fast-alt"></i>
                <span class="text">Add Student List</span>
            </div>
            
            <div>
                <div class="main" style="margin-left: 95px; align-items: center;">
                    <h2 style=" text-align:center;">Create Student Details</h2>
                        <form name="RegForm" action="/student/create" method="POST" onsubmit=" return validateform()">
                            <p>
                                <label for="first">Name:</label>
                                <input type="text" id="first" name="first" title="give the proper Name. characters only allowed" required />
                                <span id="first-error" class="error-message"></span>
                            </p>
                            <p>
                                <label for="fname">Father Name:</label>
                                <input type="text" id="fname" name="fname" title="give the proper Name. characters only allowed" required />
                                <span id="fname-error" class="error-message"></span>
                            </p>
                            <p>
                                <label for="rollno">Register Number:</label>
                                <input type="text" id="rollno" name="rollno" title="give the proper Register Number. Integer only allowed" required />
                                <span id="rollno-error" class="error-message"></span>
                            </p>
                            <p>
                                <label for="dept">Department</label>
                                <select id="dept" name="dept">
                                    <option value="None">None</option>
                                    <option value="CSE">CSE</option>
                                    <option value="IT">IT</option>
                                    <option value="EEE">EEE</option>
                                    
                                </select>
                                <span id="dept-error" class="error-message"></span>
                            </p>
                            <p>
                                <label for="year">Year</label>
                                <select id="year" name="year">
                                    <option value="None">None</option>
                                    <option value="I">I</option>
                                    <option value="II">II</option>
                                    <option value="III">III</option>
                                    <option value="IV">IV</option>
                                </select>
                                <span id="year-error" class="error-message"></span>
                            </p>
                            <p>
                                <label for="semester">Semester</label>
                                <select id="semester" name="semester">
                                    <option value="None">None</option>
                                    <option value="Odd">Odd</option>
                                    <option value="Even">Even</option>
                                </select>
                                <span id="semester-error" class="error-message"></span>
                            </p>
                            <p>
                                <label for="gender">Gender</label>
                                <select id="gender" name="gender">
                                    <option value="None">None</option>
                                    <option value="Male">Male</option>
                                    <option value="Female">Female</option>
                                    <option value="Others">Others</option>
                                </select>
                                <span id="gender-error" class="error-message"></span>
                            </p>
                            <p>
                                <label for="dateofBirth">Date of Birth</label>
                                <input type="date" id="dateofBirth" name="dateofBirth">
                                <span id="dateofBirth-error" class="error-message"></span>
                            </p>
                            <p>
                                <label for="email">Email:</label>
                                <input type="text" id="email" name="email" title="give the proper email id" required />
                                <span id="email-error" class="error-message"></span>
                            </p>
                            <p>
                                <label for="password">Password:</label>
                                <input type="password" id="password" name="password" pattern="^(?=.*\d)(?=.*[a-zA-Z])(?=.*[^a-zA-Z0-9])\S{8,}$" title="Password must contain at least one number, one alphabet, one symbol, and be at least 8 characters long" required />
                                <span id="password-error" class="error-message"></span>
                            </p>
                            <p>
                                <label for="mobile">Contact:</label>
                                <input type="text" id="mobile" name="mobile" maxlength="10" required />
                                <span id="mobile-error" class="error-message"></span>
                            </p>
                            <p>
                                <input type="submit" value="Submit" name="Submit" />
                            </p>
                        </form>
